{
  "name": "dataqna-golden-demo",
  "version": "1.0.0",
  "description": "Golden Demo for Data QnA API",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "npm install && tsc && cd client && npm install && npm run build",
    "deploy": "npm run build && gcloud app deploy",
    "dev": "concurrently \"tsc --watch\" \"NODE_ENV=development nodemon build/index.js\" \"cd client && npm start\""
  },
  "engines": {
    "npm": "10.9.2",
    "node": "22.x.x"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@grpc/grpc-js": "^1.12.6",
    "dotenv": "^16.4.7",
    "express": "^4.22.1",
    "express-ws": "^5.0.2",
    "google-auth-library": "^9.15.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.5",
    "@types/node": "^20.4.5",
    "@types/ws": "^8.5.14",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
